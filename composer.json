{
    "name": "olssonm/blockip",
    "description": "Block requests from specified IPs in Laravel 5.",
    "keywords": [
        "olssonm",
        "Laravel 5",
        "IP",
        "security",
        "middleware",
        "block ip"
    ],
    "homepage": "https://github.com/olssonm/blockip",
    "license": "MIT",
    "authors": [
        {
            "name": "Marcus Olsson",
            "email": "contact@marcusolsson.me",
            "homepage": "https://marcusolsson.me"
        }
    ],
    "require": {
        "illuminate/support": ">=5.5",
        "php" : "^7.1"
    },
    "require-dev": {
        "phpunit/phpunit" : "^6.5 || ^7.1 || ^8.0",
        "orchestra/testbench": ">=3.5.0"
    },
    "autoload": {
        "psr-4": {
            "Olssonm\\Blockip\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Olssonm\\Blockip\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.x-dev"
        },
        "laravel": {
            "providers": [
                "Olssonm\\Blockip\\BlockipServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable"
}
